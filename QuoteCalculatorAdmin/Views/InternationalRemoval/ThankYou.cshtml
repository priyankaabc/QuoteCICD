
@{
    ViewBag.Title = "ThankYou";
}

<style>
    body {
        background-color: #FFFFFF !important;
    }
    h5 {
        font-family: 'effra_mediumregular', sans-serif;
        text-align: center;
        color: #21945e;
    }
</style>

<section>
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-11 col-xl-10">
                @if (!ViewBag.IsQuickQuote)
                {
                    <h1 class="text-primary text-center mb-4"><b>@ViewBag.Survey</b></h1>
                    <h5><b>@ViewBag.ThankYouMessage</b></h5>
                }
                else
                {
                    <h5 style="color: #254899 ">
                        <b>Thank you for your booking for shipment of your personal effects. We will contact you during normal working hours to make all necessary arrangements. We look forward to being of service.</b>
                    </h5>
                }
            </div>
        </div>
    </div>
</section>